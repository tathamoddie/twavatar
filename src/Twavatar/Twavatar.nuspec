    <projectUrl>http://twavatar.codeplex.com/</projectUrl>
    <licenseUrl>http://twavatar.codeplex.com/license</licenseUrl>
  </metadata>
  <files>
    <file src="bin\Release\Twavatar.dll" target="lib" />
    <file src="bin\Release\Twavatar.pdb" target="lib" />
  </files>
</package>